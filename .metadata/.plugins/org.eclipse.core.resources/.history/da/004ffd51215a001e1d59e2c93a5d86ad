
public class LiçãoArray {

public static void main(String[] args) {
	String[] atores = new String[3];
	atores [1] = "José";
	atores [2] = "Roberto";
	atores [3] = "Agnaldo";
	
	int idades[] = new int [3];
	idades [1] = 23;
	idades [2] = 21;
	idades [3] = 24;
	
	System.out.println("nomes" + atores +"idade" + idades );
	
	System.out.println("Fim");
	
}
}
