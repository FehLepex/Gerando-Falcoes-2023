
public class LiçãoArray {

public static void main(String[] args) {
	String[] atores = new String[3];
	atores [0] = "José";
	atores [1] = "Roberto";
	atores [2] = "Agnaldo";
	
	int idades[] = new int [3];
	idades [0] = 23;
	idades [1] = 21;
	idades [2] = 24;
	
	System.out.println("nomes " + atores[0] +" idade" + idades[0] );
	
	System.out.println("Fim");
	
}
}
